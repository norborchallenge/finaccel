<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<Pattern>%-5level %thread %logger{0}.%method - %msg%n</Pattern>
		</encoder>
	</appender>

	<logger name="finaccel" level="DEBUG"/>

	<root level="WARN">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>
